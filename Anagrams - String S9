import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        String original1 = sc.next();
        String original2 = sc.next();
        
        String s1 = original1.toLowerCase();
        String s2 = original2.toLowerCase();

        if (s1.length() != s2.length()) {
            System.out.println(original1 + " and " + original2 + " are Not Anagrams.");
            return;
        }

        char[] arr1 = s1.toCharArray();
        char[] arr2 = s2.toCharArray();

        Arrays.sort(arr1);
        Arrays.sort(arr2);

        if (Arrays.equals(arr1, arr2)) {
            System.out.println(original1 + " and " + original2 + " are Anagrams.");
        } else {
            System.out.println(original1 + " and " + original2 + " are Not Anagrams.");
        }
    }
}

    
