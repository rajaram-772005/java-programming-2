import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    static int reverse(int n) {
        int rev = 0;
        while (n > 0) {
            rev = rev * 10 + (n % 10);
            n = n / 10;
        }
        return rev;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        
        int reversedNumber = reverse(n);
        
        int squareOriginal = n * n;
        int squareReversed = reversedNumber * reversedNumber;
        
        int reversedSquareOriginal = reverse(squareOriginal);
        
        if (reversedSquareOriginal == squareReversed) {
            System.out.println("Adam Number");
        } else {
            System.out.println("Not a adam number");
        }
    }
}
