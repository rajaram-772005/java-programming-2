import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int sumEven = 0;
        int productOdd = 1;
        while (n > 0) {
            int digit = n % 10;
            
            if (digit % 2 == 0) {
                sumEven += digit;
            } else {
                productOdd *= digit;
            }
            n /= 10;
        }
        System.out.println(sumEven + " " + productOdd);
    }
}
