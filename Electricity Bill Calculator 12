import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int U = sc.nextInt();
        int bill = 0;

        if (U <= 100) {
            bill = U * 5;
        } 
        else if (U <= 200) {
            bill = (100 * 5) + ((U - 100) * 7);
        } 
        else {
            bill = (100 * 5) + (100 * 7) + ((U - 200) * 10);
        }

        System.out.println(bill);
    }
}
